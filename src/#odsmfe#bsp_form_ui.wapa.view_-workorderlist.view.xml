<mvc:View controllerName="odsmfe.FormUI.controller.WorkOrderList" xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                                    
	xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" displayBlock="true" xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar"  xmlns:control="odsmfe.FormUI.control">                                                                                               
	                                                                                                                                                                                                                                                              
		<Page title="{i18n>workOrderListPageTitle} ({data>/workOrderList/count})">                                                                                                                                                                                   
			<subHeader>                                                                                                                                                                                                                                                 
				<Toolbar>                                                                                                                                                                                                                                                  
					<SearchField                                                                                                                                                                                                                                              
						search="onSearch"                                                                                                                                                                                                                                        
						width="100%" />                                                                                                                                                                                                                                          
				</Toolbar>                                                                                                                                                                                                                                                 
			</subHeader>                                                                                                                                                                                                                                                
			<content>                                                                                                                                                                                                                                                   
				<List id="idWorkOrderList"                                                                                                                                                                                                                                 
					growing="true"                                                                                                                                                                                                                                            
					growingScrollToLoad="true"                                                                                                                                                                                                                                
					inset="false"                                                                                                                                                                                                                                             
					updateFinished="onUpdateFinished"                                                                                                                                                                                                                         
					itemPress="onItemPress"                                                                                                                                                                                                                                   
					items="{                                                                                                                                                                                                                                                  
						path: '/WorkOrderSet',                                                                                                                                                                                                                                   
						sorter: {                                                                                                                                                                                                                                                
							path: 'WorkOrderNumber'                                                                                                                                                                                                                                 
						}                                                                                                                                                                                                                                                        
					}">                                                                                                                                                                                                                                                       
					<items>                                                                                                                                                                                                                                                   
						<StandardListItem                                                                                                                                                                                                                                        
							type="Navigation"                                                                                                                                                                                                                                       
							description="{Description}"                                                                                                                                                                                                                             
							title="{WorkOrderNumber}"                                                                                                                                                                                                                               
						/>                                                                                                                                                                                                                                                       
					</items>                                                                                                                                                                                                                                                  
				</List>                                                                                                                                                                                                                                                    
			</content>                                                                                                                                                                                                                                                  
		</Page>                                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    