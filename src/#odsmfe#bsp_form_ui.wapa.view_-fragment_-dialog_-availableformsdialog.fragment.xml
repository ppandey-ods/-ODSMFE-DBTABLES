<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"                                                                                                                                           
	xmlns:wt="co.uk.ondevice.FormUI.control" xmlns:ac="co.uk.ondevice.FormUI.control">                                                                                                                                                                            
	<!--<SelectDialog noDataText="No Forms Found" title="Select Form" search=".onSearch" confirm=".onDialogClose" cancel=".onDialogClose"-->                                                                                                                      
	<!--	items="{path : 'FormMetaDataSet'}">-->                                                                                                                                                                                                                   
	<!--<StandardListItem -->                                                                                                                                                                                                                                     
