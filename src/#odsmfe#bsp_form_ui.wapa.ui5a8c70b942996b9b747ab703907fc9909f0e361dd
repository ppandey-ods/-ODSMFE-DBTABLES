sap.ui.define([], function () {                                                                                                                                                                                                                                
			"use strict";                                                                                                                                                                                                                                               
			return {                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
				boolean: function (isDraft) {                                                                                                                                                                                                                              
					var sResult;                                                                                                                                                                                                                                              
					if (isDraft === "X") {                                                                                                                                                                                                                                    
						sResult = true;                                                                                                                                                                                                                                          
					} else {                                                                                                                                                                                                                                                  
						sResult = false;                                                                                                                                                                                                                                         
					}                                                                                                                                                                                                                                                         
					return sResult;                                                                                                                                                                                                                                           
				},                                                                                                                                                                                                                                                         
				button: function (mode) {                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
						var sResult;                                                                                                                                                                                                                                             
						if (mode === "Edit") {                                                                                                                                                                                                                                   
							sResult = true;                                                                                                                                                                                                                                         
						} else if (mode === "Display") {                                                                                                                                                                                                                         
							sResult = false;                                                                                                                                                                                                                                        
						}                                                                                                                                                                                                                                                        
						return sResult;                                                                                                                                                                                                                                          
					},                                                                                                                                                                                                                                                        
					IsDraft: function (isDraft) {                                                                                                                                                                                                                             
						var sResult;                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
						if (isDraft === "X") {                                                                                                                                                                                                                                   
							sResult = "YES";                                                                                                                                                                                                                                        
						} else {                                                                                                                                                                                                                                                 
							sResult = "NO";                                                                                                                                                                                                                                         
						}                                                                                                                                                                                                                                                        
						return sResult;                                                                                                                                                                                                                                          
					}                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				};                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
			});                                                                                                                                                                                                                                                         