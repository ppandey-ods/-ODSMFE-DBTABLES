	<!--description="{Version}"-->                                                                                                                                                                                                                                
	<!--/>-->                                                                                                                                                                                                                                                     
	<!--</SelectDialog>-->                                                                                                                                                                                                                                        
	<TableSelectDialog noDataText="No Forms Found" title="Select Form" search=".onSearch" confirm=".onDialogClose" cancel=".onDialogClose"                                                                                                                        
		items="{ path : 'FormMetaDataSet', sorter : { path : 'FormId', descending : false } }">                                                                                                                                                                      
		<ColumnListItem type="Navigation">                                                                                                                                                                                                                           
			<cells>                                                                                                                                                                                                                                                     
				<ObjectIdentifier title="{FormId}"/>                                                                                                                                                                                                                       
				<Text text="{Version}"/>                                                                                                                                                                                                                                   
				<ObjectNumber number="{Submitted}"/>                                                                                                                                                                                                                       
				<ObjectNumber number="{Occurances}"/>                                                                                                                                                                                                                      
			</cells>                                                                                                                                                                                                                                                    
		</ColumnListItem>                                                                                                                                                                                                                                            
		<columns>                                                                                                                                                                                                                                                    
			<Column width="20em">                                                                                                                                                                                                                                       
				<header>                                                                                                                                                                                                                                                   
					<Text text="Form ID"/>                                                                                                                                                                                                                                    
				</header>                                                                                                                                                                                                                                                  
			</Column>                                                                                                                                                                                                                                                   
			<Column minScreenWidth="Tablet" demandPopin="true">                                                                                                                                                                                                         
				<header>                                                                                                                                                                                                                                                   
					<Text text="Version"/>                                                                                                                                                                                                                                    
				</header>                                                                                                                                                                                                                                                  
			</Column>                                                                                                                                                                                                                                                   
			<Column minScreenWidth="Tablet" demandPopin="true">                                                                                                                                                                                                         
				<header>                                                                                                                                                                                                                                                   
					<Text text="No of Submissions"/>                                                                                                                                                                                                                          
				</header>                                                                                                                                                                                                                                                  
			</Column>                                                                                                                                                                                                                                                   
			<Column minScreenWidth="Tablet" demandPopin="true">                                                                                                                                                                                                         
				<header>                                                                                                                                                                                                                                                   
					<Text text="Submissions Limit"/>                                                                                                                                                                                                                          
				</header>                                                                                                                                                                                                                                                  
			</Column>                                                                                                                                                                                                                                                   
		</columns>                                                                                                                                                                                                                                                   
	</TableSelectDialog>                                                                                                                                                                                                                                          
</core:FragmentDefinition>                                                                                                                                                                                                                                     